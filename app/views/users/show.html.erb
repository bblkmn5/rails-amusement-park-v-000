<p id="notice"><%= notice %></p>

<%if @message %>
  <h2><em><%= @message %><em></h2>
<% end %>

<%= "ADMIN" if @user.admin %>

<p>Name: <%= @user.name %></p>
<p>Height: <%= @user.height %></p>
<p>Mood: <%= @user.mood %></p>
<p>Nausea Level: <%= @user.nausea %></p>
<p>Happiness Level: <%= @user.happiness %></p>
<p>Number of Tickets: <%= @user.tickets %></p>


<%= link_to "See attractions", attractions_path %><br>
<%= link_to "Log Out", '/logout' %>